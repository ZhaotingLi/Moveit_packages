<launch>
  <arg name="start_state_max_bounds_error" default="0.1" />
  <arg name="jiggle_fraction" default="0.05" />
  <arg name="arm_id" default="panda" />

  <param name="planning_plugin" value="ompl_interface/OMPLPlanner" />
  <!-- <param name="request_adapters" value="$(arg planning_adapters)" /> -->
  <param name="start_state_max_bounds_error" value="$(arg start_state_max_bounds_error)" />
  <param name="jiggle_fraction" value="$(arg jiggle_fraction)" />
  
  <node name="move_group_cpp_trajectory_generation" pkg="moveit_tutorials" type="move_group_cpp_trajectory_generation" respawn="false" output="screen">
  <rosparam command="load" file="$(find panda_moveit_config)/config/ompl_planning.yaml"/>
  </node>

</launch>
